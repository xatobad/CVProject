@model List<MvcCv.Models.Entity.TblAbout>


<section class="resume-section" id="about">
    @foreach (var item in Model)
    {
    <div class="resume-section-content">

        <h1 class="mb-0">
            @item.Name
            <span class="text-primary">@item.Surname</span>
        </h1>
        <div class="subheading mb-5">
            @item.Adress · @item.Phone ·
            <a href="@item.Mail">@item.Mail</a>
        </div>
        <p class="lead mb-5">@item.Description</p>

        @Html.Action("PartialSosyal")
    </div>
    }
    </section>


